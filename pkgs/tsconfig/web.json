{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "target": "es2022",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "isolatedDeclarations": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}

